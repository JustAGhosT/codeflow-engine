{
  "id": "continue",
  "name": "Continue",
  "description": "Open-source AI code assistant for VS Code and JetBrains IDEs",
  "category": "ai_development",
  "display_name": "Continue",
  "version": "1.0.0",
  "status": "active",
  "is_active": true,
  "is_beta": false,
  "is_deprecated": false,
  "type": "ai",
  "source": "official",
  "priority": 58,
  "min_autopr_version": "0.1.0",
  "vendor": "Continue",
  "website": "https://continue.dev",
  "pricing_model": "open_source",
  "supported_languages": [
    "javascript",
    "typescript",
    "python",
    "go",
    "java",
    "c++",
    "rust",
    "ruby",
    "php",
    "c#"
  ],
  "tags": ["code_assistant", "ai_completion", "vscode_extension", "jetbrains"],
  "documentation_url": "https://docs.continue.dev",
  "setup_guide": "https://docs.continue.dev/getting-started",
  "troubleshooting_guide": "https://docs.continue.dev/troubleshooting",
  "maintainer": "Continue",

  "detection": {
    "files": [".continue", ".continue/config.json", ".continue/config.yaml", ".continuerules"],
    "dependencies": ["continue-dev", "@continue/sdk"],
    "folder_patterns": ["continue-", "-continue", ".continue"],
    "commit_patterns": ["continue", "Continue", "continue.dev"],
    "content_patterns": ["continue.dev", "continue config"],
    "confidence_weights": {
      "files": 0.6,
      "dependencies": 0.2,
      "folder_patterns": 0.1,
      "commit_patterns": 0.05,
      "content_patterns": 0.05
    }
  },

  "project_config": {
    "primary_language": "typescript",
    "framework": "continue",
    "deployment_targets": ["local"],
    "common_files": [".continue/config.json", ".continue/config.yaml", ".continuerules"],
    "features": [
      "ai_code_completion",
      "chat_interface",
      "code_explanation",
      "refactoring_suggestions",
      "multiple_llm_support"
    ],
    "configuration_files": [".continue/config.json", ".continue/config.yaml"]
  }
}
