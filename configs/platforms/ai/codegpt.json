{
  "id": "codegpt",
  "name": "CodeGPT",
  "description": "AI-powered coding assistant plugin for VS Code with customizable AI models",
  "category": "ai_development",
  "display_name": "CodeGPT",
  "version": "1.0.0",
  "status": "active",
  "is_active": true,
  "is_beta": false,
  "is_deprecated": false,
  "type": "ai",
  "source": "official",
  "priority": 53,
  "min_autopr_version": "0.1.0",
  "vendor": "CodeGPT",
  "website": "https://codegpt.co/",
  "pricing_model": "freemium",
  "supported_languages": [
    "python",
    "javascript",
    "typescript",
    "java",
    "go",
    "c++",
    "rust",
    "php"
  ],
  "tags": ["vscode_extension", "ai_assistant", "customizable", "multi_model"],
  "documentation_url": "https://docs.codegpt.co/",
  "setup_guide": "https://docs.codegpt.co/getting-started",
  "troubleshooting_guide": "https://docs.codegpt.co/troubleshooting",
  "maintainer": "CodeGPT",

  "detection": {
    "files": [".codegpt", ".vscode/codegpt.json", "codegpt.config.json"],
    "dependencies": ["codegpt-vscode", "@codegpt/sdk"],
    "folder_patterns": ["codegpt-", "-codegpt"],
    "commit_patterns": ["codegpt", "CodeGPT", "code gpt"],
    "content_patterns": ["codegpt.co", "codegpt config"],
    "confidence_weights": {
      "files": 0.5,
      "dependencies": 0.3,
      "folder_patterns": 0.1,
      "commit_patterns": 0.05,
      "content_patterns": 0.05
    }
  },

  "project_config": {
    "primary_language": "typescript",
    "framework": "codegpt",
    "deployment_targets": ["local"],
    "common_files": [".codegpt/config.json", ".vscode/codegpt.json"],
    "features": [
      "multi_model_support",
      "code_completion",
      "chat_interface",
      "code_explanation",
      "custom_prompts"
    ],
    "configuration_files": [".codegpt/config.json", ".vscode/codegpt.json"]
  }
}
