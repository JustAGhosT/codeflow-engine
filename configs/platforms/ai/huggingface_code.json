{
  "id": "huggingface_code",
  "name": "Hugging Face Code",
  "description": "Open-source code assistance platform powered by Hugging Face's language models",
  "category": "ai_development",
  "display_name": "Hugging Face Code",
  "version": "1.0.0",
  "status": "active",
  "is_active": true,
  "is_beta": false,
  "is_deprecated": false,
  "type": "ai",
  "source": "official",
  "priority": 54,
  "min_autopr_version": "0.1.0",
  "vendor": "Hugging Face",
  "website": "https://huggingface.co/",
  "pricing_model": "open_source",
  "supported_languages": [
    "python",
    "javascript",
    "typescript",
    "java",
    "go",
    "c++",
    "rust",
    "ruby"
  ],
  "tags": ["open_source", "code_completion", "transformers", "ml_models"],
  "documentation_url": "https://huggingface.co/docs",
  "setup_guide": "https://huggingface.co/docs/getting-started",
  "troubleshooting_guide": "https://huggingface.co/docs/troubleshooting",
  "maintainer": "Hugging Face",

  "detection": {
    "files": [".huggingface", ".hf", "huggingface.config.json"],
    "dependencies": ["@huggingface/inference", "transformers"],
    "folder_patterns": ["huggingface-", "-huggingface", ".hf"],
    "commit_patterns": ["hugging face", "Hugging Face", "huggingface"],
    "content_patterns": ["huggingface.co", "hf hub"],
    "confidence_weights": {
      "files": 0.5,
      "dependencies": 0.3,
      "folder_patterns": 0.1,
      "commit_patterns": 0.05,
      "content_patterns": 0.05
    }
  },

  "project_config": {
    "primary_language": "python",
    "framework": "huggingface",
    "deployment_targets": ["local", "cloud"],
    "common_files": [".huggingface/config.json"],
    "features": [
      "model_inference",
      "code_completion",
      "custom_models",
      "open_source",
      "transformers"
    ],
    "configuration_files": [".huggingface/config.json", "huggingface.config.json"]
  }
}
